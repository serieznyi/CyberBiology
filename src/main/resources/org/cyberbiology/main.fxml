<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="600.0" minWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.cyberbiology.controller.MainController">
  <children>
      <MenuBar>
         <menus>
            <Menu mnemonicParsing="false" text="%menu.title.file">
               <items>
                  <MenuItem fx:id="menuButtonRun" mnemonicParsing="false" onAction="#actionStartApp" text="%menu.title.state.run" />
                  <MenuItem fx:id="menuButtonSetting" mnemonicParsing="false" onAction="#actionShowSettings" text="%menu.title.settings" />
                  <MenuItem fx:id="menuButtonMakeSnapshot" disable="true" mnemonicParsing="false" onAction="#actionMakeSnapshot" text="%menu.title.make_snapshot" />
                  <MenuItem fx:id="menuButtonExit" mnemonicParsing="false" onAction="#actionStopApp" text="%menu.title.exit" />
               </items>
            </Menu>
            <Menu fx:id="renderModeMenu" mnemonicParsing="false" text="Render Mode" />
         </menus>
      </MenuBar>
      <ScrollPane prefViewportHeight="550.0" prefViewportWidth="900.0" VBox.vgrow="ALWAYS">
         <content>
            <AnchorPane fx:id="mainPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <Canvas fx:id="canvas" height="200.0" width="200.0" />
               </children>
            </AnchorPane>
         </content>
      </ScrollPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
         <children>
            <Label fx:id="labelGeneration" text="Generation: 0" />
            <Separator orientation="VERTICAL" />
            <Label fx:id="labelPopulation" text="Population: 0" />
            <Separator orientation="VERTICAL" />
            <Label fx:id="labelMemory" text="Memory: 0" />
         </children>
    </HBox>
  </children>
</VBox>
